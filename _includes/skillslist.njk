<ul reversed class="skilllist"
 style="counter-reset: start-from {{ (skillslistCounter or skillslist.length) + 1 }}"
 >
{% for skill in skillslist | reverse %}
  <li class="skilllist-item{% if skill.url == url %} skilllist-item-active{% endif %}">
    <a href="{{ skill.url | url }}" class="skilllist-link">{% if skill.data.title %}{{ skill.data.title }}{% else %}<code>{{ skill.url }}</code>{% endif %}</a>
    {# <time class="skilllist-date" datetime="{{ skill.date | htmlDateString }}">{{ skill.date | readableDate }}</time> #}
    {% for tag in skill.data.tags %}
      {%- if collections.tagList.indexOf(tag) != -1 -%}
      {% set tagUrl %}/tags/{{ tag }}/{% endset %}
      <a href="{{ tagUrl | url }}" class="tag">{{ tag }}</a>
      {%- endif -%}
    {% endfor %}
  </li>
{% endfor %}
</ol>
